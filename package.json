{
  "name": "rtmp.ts",
  "version": "1.2.1",
  "description": "A TypeScript implementation of the Real Time Message Protocol (RTMP)",
  "main": "app.js",
  "scripts": {
    "build": "babel src --out-dir dist --extensions .ts",
    "start": "npm run build && node dist/init.js",
    "dev": "nodemon --exec babel-node --extensions .ts src/init.ts",
    "lint": "eslint 'src/**/*.ts'",
    "lintf": "eslint 'src/**/*.ts' --fix",
    "types": "tsc",
    "test": "mocha --exit --require ./babel-register.js test/index.ts",
    "init_git_hooks": "cp .githooks/* .git/hooks/ && chmod +x .git/hooks/*",
    "postinstall": "npm run init_git_hooks"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/node": "^7.12.6",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "@types/chai": "^4.2.14",
    "@types/chai-http": "^4.2.0",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.10",
    "@types/request-promise-native": "^1.0.17",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mitm": "^1.7.1",
    "mocha": "^8.2.1",
    "nock": "^13.0.6",
    "nodemon": "^2.0.6",
    "typescript": "^4.1.2"
  }
}
