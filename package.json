{
  "name": "rtmp.ts",
  "version": "1.2.1",
  "description": "A TypeScript implementation of the Real Time Message Protocol (RTMP)",
  "main": "src/app.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/init.js",
    "dev": "nodemon --exec babel-node --extensions .ts src/init.ts",
    "lint": "eslint 'src/**/*.ts'",
    "lintf": "eslint 'src/**/*.ts' --fix",
    "types": "tsc --noEmit",
    "test": "mocha",
    "init_git_hooks": "cp .githooks/* .git/hooks/ && chmod +x .git/hooks/*",
    "postinstall": "npm run init_git_hooks"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.3",
    "body-parser": "^1.20.2",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "request": "^2.88.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/chai-http": "^4.2.0",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mitm": "^1.7.1",
    "mocha": "^10.2.0",
    "nock": "^13.0.6",
    "nodemon": "^3.0.2",
    "tsx": "^4.7.0",
    "typescript": "^4.1.2"
  }
}
